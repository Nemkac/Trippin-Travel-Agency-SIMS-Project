<UserControl x:Class="InitialProject.WPF.View.GuestOne_Views.PDF_Report"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InitialProject.WPF.View.GuestOne_Views"
             xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"
             
             mc:Ignorable="d" 
             d:DesignHeight=" 1122.5" d:DesignWidth="793.7">
    <Grid Background="#f5f6fa">
        <Rectangle HorizontalAlignment="Center" 
            Height="80"
            Fill ="#273c75" 
            Stroke="Black"
            VerticalAlignment="Top"
            Width="930"/>
        <Rectangle HorizontalAlignment="Center" 
            Height="80"
            Fill ="#273c75" 
            Stroke="Black"
            VerticalAlignment="Top"
            Width="930" Margin="0,993,0,0"/>
        <TextBlock HorizontalAlignment="Center" 
            Height="72"
            Margin="0,987,0,0"
